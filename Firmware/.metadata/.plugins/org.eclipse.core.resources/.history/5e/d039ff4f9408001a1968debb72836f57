#include <WAdc.h>
#include <malloc.h>

float GetThermVolage(){//calib voltage 2.206
	static uint16_t prev=2000;
	uint16_t * data = ADC_ReadOvrsam(2);
	float K =0.2f;
	float result = (1.0f-K)*(data[0])+K*prev;//*40.798f);///29450.0f;
	prev =data[0];
	//result -=7.235f;
	free(data);
	return result;
}

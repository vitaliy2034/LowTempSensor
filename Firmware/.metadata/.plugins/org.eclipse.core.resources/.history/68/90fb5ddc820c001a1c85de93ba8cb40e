#include <Thermistor.h>

void  InitThermFast(float T,float R){
	calT1=T;
	calR1=R;
	calR2=-1;
}
void  InitThermAccur(float T1,float R1,float T2,float R2,float T3,float R3){
	calT1=T1;
	calT2=T2;
	calT3=T3;

	calR1=R1;
	calR2=R2;
	calR3=R3;
}
